{
    "$schema": "http://json.schemastore.org/package",
    "name": "@kayahr/jest-matchers",
    "version": "1.0.0",
    "description": "Additional Jest matchers",
    "keywords": [
        "jest",
        "matchers"
    ],
    "license": "MIT",
    "main": "lib/main/index",
    "repository": "github:kayahr/jest-matchers",
    "files": [
        "src/main",
        "lib/main"
    ],
    "funding": "https://github.com/kayahr/jest-matchers?sponsor=1",
    "scripts": {
        "clean": "rimraf lib",
        "compile": "tsc",
        "watch": "tsc -w",
        "lint": "eslint --max-warnings 0 --ext .ts src",
        "cspell": "cspell '**/*'",
        "check": "jest",
        "coverage": "jest --coverage",
        "prepare": "npm run -s clean && npm run -s compile",
        "test": "npm run -s cspell && npm run -s lint && npm run -s check"
    },
    "jest": {
        "projects": [
            {
                "displayName": "node",
                "testEnvironment": "jest-environment-node-single-context",
                "testRunner": "jest-circus/runner",
                "testMatch": [
                    "<rootDir>/lib/test/**/*.test.js"
                ]
            },
            {
                "displayName": "electron",
                "runner": "@jest-runner/electron",
                "testEnvironment": "@jest-runner/electron/environment",
                "testRunner": "jest-circus/runner",
                "testMatch": [
                    "<rootDir>/lib/test/**/*.test.js"
                ]
            }
        ],
        "collectCoverageFrom": [
            "<rootDir>/lib/main/**/*.js"
        ]
    },
    "devDependencies": {
        "@jest-runner/electron": "3.0.0",
        "@kayahr/eslint-config": "1.0.0",
        "@types/jest": "26.0.7",
        "@typescript-eslint/eslint-plugin": "3.7.1",
        "@typescript-eslint/parser": "3.7.1",
        "cspell": "4.0.63",
        "electron": "9.1.1",
        "eslint": "7.5.0",
        "eslint-plugin-deprecation": "1.1.0",
        "eslint-plugin-simple-import-sort": "5.0.3",
        "jest": "26.1.0",
        "jest-circus": "26.1.0",
        "jest-environment-node-single-context": "26.1.0",
        "rimraf": "3.0.2",
        "typescript": "3.9.7"
    },
    "dependencies": {
        "jest-matcher-utils": "^26.0.0",
        "tslib": "^2.0.0"
    }
}
