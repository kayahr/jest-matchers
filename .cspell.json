{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "version": "0.2",
    "files": [
        "**/*"
    ],
    "enabledLanguageIds": [
        "markdown",
        "typescript"
    ],
    "ignorePaths": [
        "node_modules",
        "coverage",
        "doc/api/**",
        "src/sandbox/**",
        "package-lock.json",
        "git",
        "lib",
        "dist",
        "*.log",
        "*.tgz"
    ],
    "words": [
        "cloneable",
        "cobertura",
        "deserializable",
        "deserializes",
        "Disableable",
        "downlevel",
        "Enableable",
        "fract",
        "kayahr",
        "kaylib",
        "lcov",
        "Mutex",
        "Reimer",
        "serializable",
        "tslib",
        "typedoc",
        "UUIDs",
        "xyxxxxxxxxxx",
        "yxxx"
    ],
    "language": "en",
    "enableFiletypes": [
        "javascript"
    ]
}
